/* pages/BrandHomepage/BrandHomepage.wxss */

page {
  background-color: #f5f5f5;
}

/* .scrollview {
  height: 100%;
} */

.header {
  display: grid;
  width: 100%;
  height: auto;
  grid-template-rows: 160rpx 65rpx 65rpx 100rpx 65rpx auto auto;
  grid-template-columns: 33% 34% 33%;
  grid-template-areas: 
    "search search search"
    ". logo ."
    ". logo ."
    "title title title"
    ". followbtn favornumber"
    "desc desc desc"
    "desc desc desc";
  justify-items: center;
  align-items: center;
  background-color: white;
}

.header .banner {
  grid-area: 1 / 1 / 3 / 4;
  width: 100%;
  height: 100%;
}

.header .search {
  grid-area: search;
  justify-self: stretch;
  align-self: start;
  border: 1rpx solid #777777;
  border-radius: 40rpx;
  font-size: 28rpx;
  height: 64rpx;
  line-height: 64rpx;
  padding: 0 30rpx;
  margin: 30rpx 30rpx 10rpx 30rpx;
  overflow: hidden;
  position: relative;
  opacity: 0.9;
  background: rgba(247,247,247,0.20);
  font-family: PingFangSC-Regular;
  color: rgba(255,255,255,0.70);
  letter-spacing: 0;
}

.header .logo {
  grid-area: logo;
  display: flex;
  justify-content: center;
  width: 130rpx;
  height: 130rpx;
  margin: 0 0 0 0;
  border-radius: 65rpx;
  overflow: hidden;
  box-shadow: 0 10rpx 40rpx #eee;
  background-color: #fff;
}

.header .logo image {
  width: 90%;
  height: 90%;
  align-self: center;
}

.header .title {
  grid-area: title;
  width: 80%;
  height: 80rpx;
  margin: 20rpx 0;
  text-align: center;
  color: #222;
	font-weight: 500;
  font-size: 38rpx;
  line-height: 40rpx;
  display: block;
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}

.header .followbtn {
  grid-area: followbtn;
  width: 220rpx;
  height: 64rpx;
  border-radius: 45rpx;
  background: #ffd721;
  font-size: 24rpx;
  color: #000;
  display: block;
  position: relative;
  margin: 0 auto;
  text-align: center;
  line-height: 64rpx;
}

.header .followedbtn {
  grid-area: followbtn;
  width: 220rpx;
  height: 64rpx;
  border-radius: 45rpx;
  background: #F8F8F8;
  font-size: 24rpx;
  color: #000;
  margin: 0 auto;
  text-align: center;
  line-height: 64rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.header .followedbtn .icon {
  width: 30rpx;
  height: 30rpx;
  margin: 0 10rpx 0 0;
  background: center url(data:image/png;base64,);
  background-size: 100%; 
}

.header .followedbtn .label {
  color: rgba(51,51,51,0.30);
}

.header .favornumber {
  grid-area: favornumber;
  justify-self: start;
  font-size: 22rpx;
  color: #c1c1c1;
}

.header .desc {
  grid-area: desc;
  display: flex;
  flex-direction: column;
  padding: 40rpx 0;
}

.header .desc .content {
  grid-area: desc;
  display: flex;
  flex-direction: row;
}

.header .desc .quotation {
  width: 30rpx;
  height: 20rpx;
  margin: 8rpx 0 0 0;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
}

.header .desc .descthumb {
  grid-area: desc;
  display: block;
  display: -webkit-box;
  margin: 0 20rpx;
  width: 440rpx;
  height: 80rpx;
  font-size: 24rpx;
  color: #c1c1c1;
  line-height: 40rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}

.header .desc .descdetail {
  grid-area: desc;
  display: block;
  display: -webkit-box;
  margin: 0 20rpx;
  width: 440rpx;
  height: 400rpx;
  font-size: 24rpx;
  color: #c1c1c1;
  line-height: 40rpx;
  overflow: hidden;
  text-overflow: ellipsis;
}

.header .desc .expand {
  align-self: flex-end;
  width: 15rpx;
  height: 30rpx;
  margin: 50rpx 0 0 0rpx;
  background: url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABAAAAAICAYAAAGHcU8BAAAAAXNSR0IArs4c6QAAAHdJREFUKBWFTwESwBAMq7d46Hiot5i4plqzzR2qSZOQ1loZuyccwsUu3hFxjM6RhCZGxnWh9ivnPHHreW02zx5ERSotVkQHDotiHgyleAW4uFqBpIENowXYj18Y61xMF4sAjqr/CYV4QYBGL0JhkNzPG0L7n/eBG3LPUFu1pVhCAAAAAElFTkSuQmCC) no-repeat;
  background-size: 100%;
}

.header .desc .collapse {
  align-self: center;
  width: 15rpx;
  height: 30rpx;
  margin: -20rpx 0 0 0;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
}

.content {
  margin: 20rpx 0 0 0;
}

.content .tabs {
  display: grid;
  position: sticky;
  top: 0;
  width: 100%;
  height: 90rpx;
  grid-template-rows: 90rpx;
  grid-template-columns: 20% 20% 20% 20% 20%;
  justify-items: center;
  align-items: center;
  font-size: 28rpx;
  color: #bbb;
  background-color: #fff;
  padding: 0 0;
  /* border-bottom: 1rpx solid #e9e9e9; */
  font-weight: bold;
  z-index: 99;
}

.content .tabs .price {
  display: flex;
  justify-content: flex-end;
}

.content .tabs .price .icon {
  width: 18rpx;
  height: 30rpx;
  margin: 5rpx 5rpx 0 15rpx;
  background: center url(data:image/png;base64,) no-repeat;
    background-size: 100%;
}

.content .tabs .filter {
  justify-self: stretch;
  display: flex;
  justify-content: space-between;
}

.content .tabs .filter .border {
  width: 3rpx;
  height: 30rpx;
  background: #bbb;
  margin: 6rpx 0 0 0;
}

.content .tabs .filter .hidden-border {
  width: 1rpx;
  height: 30rpx;
  background: #fff;
  margin: 6rpx 0 0 0;
}

.content .tabs .active {
  color: #333;
}

.content .list {
  padding: 0 0;
}

.content .list .item-row {
  background: #fff;
}

.content .list .goods-item {
  display: grid;
  width: auto;
  height: 180rpx;
  background: #fff;
  padding: 30rpx 0;
  margin: 0 30rpx;
  box-shadow: inset 0 -1px 0 0 #E9E9E9;
  grid-template-rows: auto;
  grid-template-columns: 180rpx auto;
  grid-template-areas: "thumb overview"; 
}

.content .list .goods-item .thumb {
  width: 180rpx;
  height: 180rpx;
  margin: 0 0 0 0;
  grid-area: thumb;
  background: #F8F8F8;
  border: 1px solid #EEEEEE;
  border-radius: 8rpx; 
}

.content .list .goods-item .thumb .cornermark {
  position: relative;
  top: 0;
  left: 0;
  border-top-left-radius: 8rpx;
  width: 50%;
  height: 30rpx;
  background: #FFD721;
  font-family: PingFangSC-Regular;
  font-size: 18rpx;
  text-align: center;
  line-height: 30rpx;
  color: #000;
  letter-spacing: 0;
}

.content .list .goods-item .overview {
  width: auto;
  height: auto;
  grid-area: overview;
  margin: 0 0 0 30rpx;
  overflow: hidden;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.content .list .goods-item .overview .title {
  display: block;
  display: -webkit-box;
  width: auto;
  height: 58rpx;
  font-family: PingFangSC-Medium;
  font-size: 24rpx;
  color: #333;
  letter-spacing: 0;
  line-height: 26rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  margin: 5rpx 0 1rpx 0;
}

.content .list .goods-item .overview .labels {
  display: flex;
  flex-direction: row;
}

.content .list .goods-item .overview .labels .label {
  width: 80rpx;
  height: auto;
  text-align: center;
  font-family: PingFangSC-Medium;
  font-size: 20rpx;
  color: #fff;
  letter-spacing: 0;
  background: #FF5151;
  border-radius: 6rpx;
}

.content .list .goods-item .overview .sale-price {
  font-family: SFUIText-Medium;
  font-size: 24rpx;
  color: #333;
  letter-spacing: 0;
}

.content .list .goods-item .overview .market-price {
  font-family: PingFangSC-Medium;
  font-size: 20rpx;
  color: #bbb;
  letter-spacing: 0;
}

.content .list .news-item {
  width: 100%;
  height: 240rpx;
  background-image: linear-gradient(-180deg, #F8F8F8 2%, #FFFFFF 100%);
}

.content .list .news-item .bgrd {
  display: grid;
  width: auto;
  height: 180rpx;
  padding: 30rpx 0;
  margin: 0 30rpx;
  box-shadow: inset 0 -1px 0 0 #E9E9E9;
  grid-template-rows: auto;
  grid-template-columns: 180rpx auto;
  grid-template-areas: "poster desc"; 
}

.content .list .news-item .bgrd .poster {
  width: 180rpx;
  height: 180rpx;
  margin: 0 30rpx 0 0;
  grid-area: poster;
  border-radius: 8rpx; 
}

.content .list .news-item .bgrd .poster .cornermark {
  position: relative;
  top: 0;
  left: 0;
  border-top-left-radius: 8rpx;
  width: 50%;
  height: 30rpx;
  background: #FFD721;
  font-family: PingFangSC-Regular;
  font-size: 18rpx;
  text-align: center;
  line-height: 30rpx;
  color: #000;
  letter-spacing: 0;
}

.content .list .news-item .bgrd .desc {
  width: auto;
  height: auto;
  grid-area: desc;
  margin: 0 0 0 0;
  overflow: hidden;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.content .list .news-item .bgrd .desc .brandname {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 400rpx;
}

.content .list .news-item .bgrd .desc .brandname .b1 {
  width: 35rpx;
  height: 2rpx;
  background: #bbb;
}

.content .list .news-item .bgrd .desc .brandname .b2 {
  margin: 0 25rpx;
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #bbb;
  letter-spacing: 0;
}

.content .list .news-item .bgrd .desc .title {
  font-family: PingFangSC-Semibold;
  font-size: 30rpx;
  color: #333333;
  letter-spacing: 0;
  margin: 20rpx;
  display: block;
  display: -webkit-box;
  width: 400rpx;
  height: 60rpx;
  line-height: 30rpx;
  overflow: hidden;
  text-align: center;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}

.content .list .news-item .bgrd .desc .period {
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #bbb;
  letter-spacing: 0;
}

.content .nomore {
  display: flex;
  width: 100%;
  height: 130rpx;
  justify-content: center;
  align-items: center;
}

.content .nomore .e1 {
  width: 70rpx;
  height: 2rpx;
  background-color: #bbb;
}

.content .nomore .label {
  font-family: PingFangSC-Medium;
  font-size: 24rpx;
  color: rgba(51,51,51,0.20);
  letter-spacing: 0;
  margin: 0 40rpx;
}

